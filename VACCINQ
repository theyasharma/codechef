#include <bits/stdc++.h>
using namespace std;

int main() 
{
    int t;
    int arr[100];
    cin>>t;
    while(t--){
      int n,p,x,y,tim;
      cin>>n>>p>>x>>y;
      for(int i=0;i<n;i++){
        cin>>arr[i];
      }
      for(int i=0;i<p;i++){
        if(arr[i]==0){
          tim+=x;
        }else if(arr[i]==1){
          tim+=y;
        }
      }
      cout<<tim<<endl;
      tim=0;
    }
    
}
