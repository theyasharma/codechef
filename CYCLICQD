#include <bits/stdc++.h>
using namespace std;

int main() 
{
    int t;
    cin>>t;
    while(t--){
      int a,b,c,d;
      cin>>a>>b>>c>>d;
      if((a+c)==180){
        if((b+d)==180){
          cout<<"yes"<<endl;
        }else{
          cout<<"no"<<endl;
        }
      }else{
          cout<<"no"<<endl;
        }
      
      
    }
}
